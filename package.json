{
  "name": "enfuroped-api",
  "scripts": {
    "start": "nodemon --watch \"src/\" --exec \"npm run lint:src && npx ts-node src/index.ts\" -e ts",
    "start:build": "node build/index.js",
    "test": "npm run lint:tests && npm run migrate_reset_test && npx dotenv -e env/.test.env jest tests -- --runInBand",
    "test:no-prepare": "npx dotenv -e env/.test.env jest tests -- --runInBand",
    "docs": "node scripts/serveDocs.js",
    "ci": "npm run lint:tests && npm run migrate_ci && npx dotenv -e env/.ci.env jest",
    "build": "rimraf build && tsc -p tsconfig.build.json",
    "format": "prettier --write ./src/**/*.ts",
    "prisma_studio_dev": "dotenv -e env/.dev.env prisma studio",
    "prisma_studio_test": "dotenv -e env/.test.env prisma studio",
    "prisma_studio_deploy": "dotenv -e env/.build.env prisma studio",
    "prisma_studio_container": "prisma studio",
    "lint:src": "eslint \"src/**\"",
    "lint:tests": "eslint \"tests/**\"",
    "migrate_dev": "dotenv -e env/.dev.env prisma migrate dev",
    "migrate_test": "dotenv -e env/.test.env prisma migrate deploy",
    "migrate_ci": "dotenv -e env/.ci.env prisma migrate deploy",
    "migrate_deploy": "dotenv -e env/.build.env prisma migrate deploy",
    "migrate_container": "prisma migrate deploy",
    "migrate_reset_dev": "dotenv -e env/.dev.env prisma migrate reset",
    "migrate_reset_test": "dotenv -e env/.test.env prisma migrate reset -- --force --skip-generate"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@quixo3/prisma-session-store": "^3.1.10",
    "bcrypt": "^5.1.0",
    "date-fns": "^2.29.3",
    "dotenv-cli": "^6.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "node-cpf": "^1.1.1",
    "nodemailer": "^6.8.0",
    "nodemailer-express-handlebars": "^5.0.0",
    "nodemailer-html-to-text": "^3.2.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "prisma": "^4.5.0",
    "prisma-docs-generator": "^0.5.0",
    "qs": "^6.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.2.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.3",
    "@types/nodemailer": "^6.4.6",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/nodemailer-html-to-text": "^3.1.0",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.1",
    "jest-environment-node": "^29.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "description": "[![EnfUroped_API CI](https://github.com/FCR-group/EnfUroped-API/actions/workflows/githubActions.yml/badge.svg)](https://github.com/FCR-group/EnfUroped-API/actions/workflows/githubActions.yml)",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FCR-group/EnfUroped-API.git"
  },
