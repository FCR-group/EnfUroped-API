{
  "name": "enfuroped-api",
  "scripts": {
    "start": "nodemon --watch \"src/\" --exec \"npm run lint:src && npx ts-node src/index.ts\" -e ts",
    "test": "npm run lint:tests && npx dotenv -e env/.test.env jest tests -- --runInBand",
    "docs": "node scripts/serveDocs.js",
    "build": "rimraf build && tsc -p tsconfig.build.json",
    "format": "prettier --write ./src/**/*.ts",
    "lint:src": "eslint \"src/**\"",
    "lint:tests": "eslint \"tests/**\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv-cli": "^6.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.1",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.2.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.3",
    "@types/node-fetch": "^2.6.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.2.1",
    "jest-environment-node": "^29.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
